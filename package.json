{
  "name": "atomdoc-cli",
  "description": "A CLI interface for AtomDoc",
  "version": "1.0.6",
  "bin": {
    "atomdoc": "./bin/index.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d ./ ./src/",
    "test": "ava",
    "coverage": "nyc npm test",
    "lint": "eslint .",
    "codecov": "nyc report -r lcovonly && codecov",
    "checkdocs": "atomdoc src",
    "validate": "npm run lint && npm run coverage && npm run checkdocs",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/atomdoc-cli.git"
  },
  "keywords": [
    "AtomDoc",
    "cli",
    "javascript",
    "documentation"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/atomdoc-cli/issues"
  },
  "homepage": "https://github.com/GarthDB/atomdoc-cli#readme",
  "eslintConfig": {
    "extends": "garthdb"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/lib/*.js",
      "bin/index.js",
      "lib/basic_reporter.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "acorn": "^4.0.8",
    "atomdoc": "^1.0.4",
    "chalk": "^1.1.3",
    "cliff": "^0.1.10",
    "commander": "^2.9.0",
    "falafel": "^2.0.0",
    "fs-readfile-promise": "^3.0.0",
    "glob": "^7.1.1",
    "pkginfo": "^0.4.0",
    "resolve": "^1.2.0",
    "tello": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "codecov": "^1.0.1",
    "cz-customizable": "^4.0.0",
    "eslint": "^3.15.0",
    "eslint-config-garthdb": "^0.1.0",
    "nixt": "^0.5.0",
    "nyc": "^10.1.2",
    "semantic-release": "^6.3.2"
  }
}
