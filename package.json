{
  "name": "atomdoc-cli",
  "description": "A CLI interface for AtomDoc",
  "version": "0.0.0",
  "bin": {
    "atomdoc": "./bin/index.js"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d ./ ./src/",
    "test": "nyc ava",
    "coverage": "nyc npm test",
    "lint": "eslint .",
    "codecov": "nyc report -r lcovonly && codecov",
    "validate": "npm run lint && npm run test",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarthDB/atomdoc-cli.git"
  },
  "keywords": [
    "AtomDoc",
    "cli",
    "javascript",
    "coffeescript",
    "documentation"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/atomdoc-cli/issues"
  },
  "homepage": "https://github.com/GarthDB/atomdoc-cli#readme",
  "eslintConfig": {
    "extends": "garthdb"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/lib/*.js",
      "bin/index.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "acorn": "^4.0.2",
    "atomdoc": "^1.0.4",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "falafel": "^2.0.0",
    "fs-readfile-promise": "^3.0.0",
    "pkginfo": "^0.4.0",
    "resolve": "^1.1.7",
    "tello": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "codecov": "^1.0.1",
    "eslint": "^3.4.0",
    "eslint-config-garthdb": "0.1.0",
    "nixt": "^0.5.0",
    "nyc": "^8.1.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
